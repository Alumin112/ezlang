ez is_prime(n: int) -> bool {
    if (n % 2 == 0)
        return false
    for (let i = 3 : i * i <= n : i += 2) 
        if (n % i == 0)
            return false
    return true
}

ez get_prime(n: int) -> int {
    let current = 0
    for (let i = 2 : true : ++i)
        if (is_prime(i))
            current += 1
            if (current == n)
                return i
}
