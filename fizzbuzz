ez fizzbuzz(start: int, end: int) {
    for (let i = start : i <= end : ++i) {
        let none = false
        if (i % 3 == 0) {
            ezout 'f', 'i', 'z', 'z'
            none = true
        }
        if (i % 5 == 0) {
            ezout 'b', 'u', 'z', 'z'
            none = true
        }
        if (none) {
            ezout '\n'
        } else {
            ezout i, '\n'
        }
    }
}
