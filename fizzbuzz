ez fizzbuzz(start: int, end: int) {
    end += 1
    for (int i = start i < end ++i) {
        bool none = false
        if i % 3 == 0 {
            ezout 'f', 'i', 'z', 'z'
            none = true
        }
        if i % 5 == 0 {
            ezout 'b', 'u', 'z', 'z'
            none = true
        }
        if (none) {
            ezout '\n'
        } else {
            ezout i, '\n'
        }
    }
}
